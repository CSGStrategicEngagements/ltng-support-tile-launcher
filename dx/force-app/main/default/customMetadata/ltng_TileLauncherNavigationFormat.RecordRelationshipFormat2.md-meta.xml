        <value xsi:type="xsd:string">{
    &#39;type&#39;: &#39;standard__recordRelationshipPage&#39;,
    &#39;attributes&#39;: {
        &#39;recordId&#39;: &#39;&lt;%=_1%&gt;&#39;,
        &#39;relationshipApiName&#39;: &#39;&lt;%=_2%&gt;&#39;,
        &#39;actionName&#39;: &#39;&lt;%=_3%&gt;&#39;
    }
}</value>
