<?xml version="1.0" encoding="UTF-8"?>
<FlexiPage xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>User Friendly list of tiles that can launch different apps or navigate within Salesforce</description>
    <flexiPageRegions>
        <componentInstances>
            <componentInstanceProperties>
                <name>height</name>
                <value>200px</value>
            </componentInstanceProperties>
            <componentInstanceProperties>
                <name>targetAddress</name>
                <value>/lightning/setup/SetupOneHome/home</value>
            </componentInstanceProperties>
            <componentInstanceProperties>
                <name>width</name>
                <value>50%</value>
            </componentInstanceProperties>
            <componentName>ltng_HeroButton</componentName>
        </componentInstances>
        <name>main</name>
        <type>Region</type>
    </flexiPageRegions>
    <masterLabel>Tile Launcher</masterLabel>
    <template>
        <name>flexipage:defaultAppHomeTemplate</name>
    </template>
    <type>AppPage</type>
</FlexiPage>
